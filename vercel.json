{
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api"
    },
    {
      "src": "/vapid-public-key",
      "dest": "/api"
    },
    {
      "src": "/subscribe",
      "dest": "/api"
    },
    {
      "src": "/unsubscribe",
      "dest": "/api"
    },
    {
      "src": "/send-notification",
      "dest": "/api"
    },
    {
      "src": "/devices",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
